<tr>
  <td><%= link_to dtrips.id, trip_path(dtrips.id) %></td>
  <td><%= dtrips.date %></td>
  <td><%= link_to dtrips.passenger_id, passenger_path(dtrips.passenger_id) %></td>
  <% if dtrips.in_progress == true %>
    <td id='in_progress'> <i class="w3-xxlarge material-icons w3-spin icons">refresh</i></td>
  <% else %>
    <td id='complete'> <i class="w3-xxlarge material-icons icons" >check_circle_outline</i></td>
  <% end %>
  <td><%= dtrips.rating %></td>
  <td>$<%= '%.2f' % (dtrips.cost / 100) %></td>
</tr>
