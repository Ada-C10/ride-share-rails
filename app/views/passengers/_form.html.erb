<h1> <%= action_name %> </h1>

<%= form_with model: @passenger, class: "passenger-form" do |f|%>
  <p>Please provide the following information:</p>

  <%= f.label :name %>
  <%= f.text_field :name %>

  <%= f.label :phone_num %>
  <%= f.text_field :phone_num %>


  <%= f.submit button_title, class: "passenger-button" %>
<% end %>
