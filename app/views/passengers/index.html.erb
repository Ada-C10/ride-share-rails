<h1>List of Passengers</h1>
<% @passengers.each do |passenger| %>
      <a href= "/passengers/ <%=passenger.id %>">
          <%= passenger[:name] %>
        </a>
      </h2>
      <ul>
        <li>Phone Number: <%= passenger[:phone_num] %></li>
      </ul>
      <ul>
      <li><%= link_to "Edit", edit_passenger_path(passenger.id)%></li>
      <li><%= link_to "Delete #{passenger.name}?", passenger_path(passenger.id), method: :delete, data: {confirm:"Are you sure you want to delete?"} %></li>
      </ul>
      <%end%>
