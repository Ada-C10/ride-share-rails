<h1>List of passengers</h1>

<section>
  <ul>
    <li>Name</li>
    <li>Phone #</li>
  </ul>

  <ul>
    <% Passenger.active_passenger.each do |passenger| %>
    <li>
      <p><%= link_to "#{passenger.name}", passenger_path(passenger.id) %></p>
    </li>
    <li>
      <p><%= passenger.phone_num %></p>
    </li>

    <% end %>
  </ul>
</section>
