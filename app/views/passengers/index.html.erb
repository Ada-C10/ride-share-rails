<h1>All Passengers</h1>


  <% @passengers.each do |p| %>
    <%= link_to p.name, passenger_trips_path(p.id) %>
    <%= link_to "Edit", edit_passenger_path(p.id) %>
    <%= link_to "Delete", passenger_path(p.id), method: :delete,
     data: { confirm: "Are you sure you want to delete this passenger?" } %>
    <br>
    <%= p.phone_num %>
    <br>
    <br>

  <% end %>
