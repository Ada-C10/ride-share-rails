
<table id="index_table">
    <tr>
      <th>PASSENGER NAME</th>
      <th>PASSENGER ID</th>
      <th>EDIT ACTIONS</th>
    </tr>
    <tbody>
<tr>
<% active_passengers = @passengers.where(active: true) %>
<% active_passengers.each do |passenger| %>
  <tr>
    <td>
      <%= link_to "#{passenger.name}", passenger_path(passenger.id) %>
    </td>
    <td>
      <%= link_to "#{passenger.id}", passenger_path(passenger.id) %>
    </td>
  
  </tr>
<%end%>

</table>

