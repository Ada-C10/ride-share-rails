<body class="passenger_index">
  <h1>List of Passengers</h1>

  <% active_passengers = @passengers.where(active: true) %>

  <table class="index_table">
      <tr>
        <th>PASSENGER NAME</th>
        <th>PASSENGER ID</th>
      </tr>
      <tbody>
        <%= render partial: "passenger", collection: active_passengers %>
      </tbody>
  </table>
</body>
