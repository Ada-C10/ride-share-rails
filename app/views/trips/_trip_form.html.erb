<%= form_with model: @trip, class: 'trip_form' do |f| %>

  <h3><%= form_title %></h3>

  <%= f.label :date %>
  <%= f.text_field :date %>

  <%= f.label :rating %>
  <%= f.text_field :rating %>

  <%= f.label :cost %>
  <%= f.text_field :cost %>

  <%= f.submit action_type, class: "trips-form-submit-button" %>

<% end %>
