<h1>Edit Trip</h1>

<%= form_with model: @trip do |f|%>
  <%= f.label :date %>
  <%= f.text_field :date %>
  <br />
  <%= f.label :passenger_id %>
  <%= f.text_field :passenger_id %>
  <br />
  <%= f.label :driver_id %>
  <%= f.text_field :driver_id %>
  <br />
  <%= f.label :cost %>
  <%= f.text_field :cost %>
  <br />
  <%= f.label :rating %>
  <%= f.text_field :rating %>
  <br />
  <%= f.submit %>
<% end %>
