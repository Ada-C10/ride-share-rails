<%= form_with model: @trip do |f|%>
<h1>Edit Trip<h1>
  <%= f.label :date , "Date:"%>
  <%= f.text_field :date%>
  <br>
  <%= f.label :driver_id , "Driver:"%>
  <%= f.text_field :driver_id%>
  <br>
  <%= f.label :passenger , "Passenger:"%>
  <%= f.text_field :passenger%>
  <br>
  <%= f.label :cost , "Cost:"%>
  <%= f.text_field :cost%>
  <br>
  <%= f.label :rating , "Rating:"%>
  <%= f.text_field :rating%>

  <br>
  <%= f.submit "Update Trip"%>
<%end%>
