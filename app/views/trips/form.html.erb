<h1> <%= action_name %> </h1>


<%= form_with model: @Trip, class: "trip-form" do |f|%>
  <p>Please provide the following information:</p>

  <%= f.label :name %>
  <%= f.text_field :name %>

  <%= f.label :phone_number %>
  <%= f.text_field :phone_number %>



  <%= f.submit "Submit!", class: "trip-button" %>
<% end %>
